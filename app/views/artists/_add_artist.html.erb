<div class=add_artist>
  <div>
    <%= form_for(@artist) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>
      <%= f.label "Add a new artist" %>
      <%= f.text_field :name, class:       'form-control' %>
      <%= f.submit "Add", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
